AA,aAHS,GAGO,KAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,aAH1B;AAIT,gBAAA,YAJS,GAIM,KAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,YAJzB;AAAA;AAAA,uBAMT,KAAI,CACL,GADC,4BACuB,aADvB,cACwC,YADxC,GAED,IAFC,CAEI,UAAA,QAAO,EAAK;AACd,kBAAA,KAAI,CAAC,OAAL,GAAe,QAAQ,CAAC,IAAxB;AACA,kBAAA,QAAQ,CAAC,KAAT,GAAiB,KAAI,CAAC,OAAL,CAAa,IAAb,GAAoB,WAArC;AACH,iBALC,EAMD,KANC,CAMK,UAAA,KAAI,EAAK;AACZ,kBAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACH,iBARC,CANS;;AAAA;AAef,gBAAA,KAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,cAAnB,EAAmC,KAAnC;;AAfe;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBlB,KAjBI;AAkBL,IAAA,SAlBK,uBAkBO;AACR,UAAG,KAAK,CAAC,KAAK,QAAN,CAAL,IAAwB,KAAK,QAAL,GAAgB,CAA3C,EAA8C;AAC1C,aAAK,QAAL,GAAgB,CAAhB;AACJ;;AACA,UAAM,IAAG,GAAI;AACT,QAAA,OAAO,EAAC,KAAK,OADJ;AAET,QAAA,QAAQ,EAAE,KAAK;AAFN,OAAb;AAIA,WAAK,MAAL,CAAY,MAAZ,CAAmB,WAAnB,EAAgC,IAAhC;AAEA,MAAA,KAAK,CAAC;AACF,QAAA,OAAO,EAAE,mCADP;AAEF,QAAA,IAAI,EAAE,YAFJ;AAGF,QAAA,WAAW,EAAE,IAHX;AAIF,QAAA,YAAY,EAAE,IAJZ;AAKF,QAAA,QAAQ,EAAE,IALR;AAMF,QAAA,QAAQ,EAAE;AANR,OAAD,CAAL;AAQJ;AApCK;AA/BE,CAAf","sourcesContent":["<template>\r\n    <div class=\"product-detail card-wrapper padding-top padding-half-bottom\">\r\n        <div class=\"container\">\r\n            <div class=\"product-header\">\r\n                <div class=\"row\">\r\n\r\n                    <div class=\"col-xl-6 col-lg-6 col-md-6 col-sm-6 col-xs-12 col-12 product-img\">\r\n                        <div class=\"img-display\">\r\n                            <div class=\"img-showcase\" id=\"showcase\">\r\n                                    <img\r\n                                        v-for=\"item in 4\" \r\n                                        v-bind:key=\"item\"\r\n                                        v-bind:src=\"product.get_image\" alt=\"\" class=\"img\">\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"row img-select\">\r\n                            <div class=\"img-item col-lg-3 col-md-6 col-sm-6 col-6\">\r\n                                <a href=\"#\" data-id=\"1\">\r\n                                    <img v-bind:src=\"product.get_thumbnail\" alt=\"\">\r\n                                </a>\r\n                            </div>\r\n                            <div class=\"img-item col-lg-3 col-md-6 col-sm-6 col-6\">\r\n                                <a href=\"#\" data-id=\"2\">\r\n                                    <img v-bind:src=\"product.get_thumbnail\" alt=\"\">\r\n                                </a>\r\n                            </div>\r\n                            <div class=\"img-item  col-lg-3 col-md-6 col-sm-6 col-6\">\r\n                                <a href=\"#\" data-id=\"3\">\r\n                                    <img v-bind:src=\"product.get_thumbnail\" alt=\"\">\r\n                                </a>\r\n                            </div>\r\n                            <div class=\"img-item  col-lg-3 col-md-6 col-sm-6 col-6\">\r\n                                <a href=\"#\" data-id=\"4\">\r\n                                    <img v-bind:src=\"product.get_thumbnail\" alt=\"\">\r\n                                </a>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <!-- card right -->\r\n                    <div class=\"col-xl-6 col-lg-6 col-sm-6 col-xs-12 col-12 product-content\">\r\n                        <h2 class=\"product-title\">{{ product.name }}</h2>\r\n                        <a class=\"product-link\">visit ____ store</a>\r\n                        <div class=\"product-rating\">\r\n                            <i class=\"fas fa-star\"></i>\r\n                            <i class=\"fas fa-star\"></i>\r\n                            <i class=\"fas fa-star\"></i>\r\n                            <i class=\"fas fa-star\"></i>\r\n                            <i class=\"fas fa-star-half-alt\"></i>\r\n                            <span>4.7(21)</span>\r\n                        </div>\r\n\r\n                        <div class=\"product-price\">\r\n                            <!--<p class=\"last-price\">Old Price: <span>$257.00</span></p>-->\r\n                            <p class=\"new-price\">New Price: <span>&dollar;{{ product.price }}</span></p>\r\n                        </div>\r\n\r\n                        <div class=\"product-detail\">\r\n                            <h2>about this item: </h2>\r\n                            <p>{{ product.description }}</p>\r\n                            <ul>\r\n                                <li>Color: <span>{{ product.color }}</span></li>\r\n                                <li>Available: <span>in stock</span></li>\r\n                                <li>Shipping Area: <span>All over the world</span></li>\r\n                                <li>Shipping Fee: <span>Free</span></li>\r\n                            </ul>\r\n                        </div>\r\n\r\n                        <div class=\"purchase-info\">\r\n                            <input type=\"number\" min=\"1\" v-model=\"quantity\">\r\n                            <button type=\"button\" class=\"btn\" @click=\"addToCart\">\r\n                                Add to Cart <i class = \"fas fa-shopping-cart\"></i>\r\n                            </button>\r\n                            <button type=\"button\" class=\"btn\">Compare</button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport '../assets/css/product.css'\r\nimport axios from 'axios'\r\nimport {toast } from 'bulma-toast'\r\n\r\nexport default {\r\n    name: \"Product\",\r\n    data() {\r\n        return {\r\n            product: {},\r\n            quantity: 1\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getProduct()\r\n\r\n        const imgs = document.querySelectorAll('.img-select a');\r\n        const imgBtns = [...imgs];\r\n        let imgId = 1;\r\n\r\n        imgBtns.forEach((imgItem) => {\r\n            imgItem.addEventListener('click', (event) => {\r\n                event.preventDefault();\r\n                imgId = imgItem.dataset.id;\r\n                slideImage();\r\n            });\r\n        });\r\n\r\n        function slideImage() {\r\n            const displayWidth = document.querySelector('.img-showcase img:first-child').clientWidth;\r\n\r\n            document.getElementById('showcase').style.transform = `translateX(${- (imgId - 1) * displayWidth}px)`;\r\n        }\r\n\r\n        window.addEventListener('resize', slideImage);\r\n    },\r\n    methods: {\r\n        async getProduct() {\r\n            this.$store.commit('setIsLoading', true)\r\n\r\n            const category_slug = this.$route.params.category_slug\r\n            const product_slug = this.$route.params.product_slug\r\n\r\n            await axios\r\n                .get(`/api/v1/products/${category_slug}/${product_slug}`)\r\n                .then(response => {\r\n                    this.product = response.data\r\n                    document.title = this.product.name + ' | Random'\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                })\r\n            this.$store.commit('setIsLoading', false)\r\n        },\r\n        addToCart() {\r\n            if(isNaN(this.quantity) || this.quantity < 1) {\r\n                this.quantity = 1\r\n            }\r\n            const item = {\r\n                product:this.product,\r\n                quantity: this.quantity\r\n            }\r\n            this.$store.commit('addToCart', item)\r\n\r\n            toast({\r\n                message: 'The product was added to the cart',\r\n                type: 'is-success',\r\n                dismissible: true,\r\n                pauseOnHover: true,\r\n                duration: 2000,\r\n                position: 'bottom-right',\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":""}]}